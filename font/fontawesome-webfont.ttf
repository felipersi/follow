                <div class="input-icon left">
                    <i class="icon-user"></i>
                    <input type="text" class="m-wrap placeholder-no-fix valid" placeholder="Login" name="login" id="login" />
                </div>
            </div>
        </div>

        <div class="control-group">
            <div class="controls">
                <div class="input-icon left">
                    <i class="icon-lock"></i>
                    <input type="password" class="m-wrap placeholder-no-fix" placeholder="Senha" name="senha" id="senha" />
                </div>
            </div>
        </div>
        <div class="form-actions">
            <input type="submit" name="submit" id="submit" class="btn btn-primary" value="Entrar" />        
        </div>

        <div class="message-box">
                    </div>
