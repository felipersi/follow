	<script>
	  $(document).ready(function () {
		  $('.bugTabs').live("click", function(){
		  	var divWidth = $('.bugsWrap .textarea-div').width();
      
		  	if($(this).hasClass('listOpen') && $(this).not('.selected')){
		          $('.bugsWrap').animate({
		  			left: '-' + (divWidth) + 'px'
		          }, 200);
		          $('.bt-saveResp').hide();
		      }else if($(this).hasClass('listClose') && $(this).not('.selected')){
		          $('.bugsWrap').animate({
		          	left: '-' + (divWidth * 2) + 'px'
		          }, 200);
		          $('.bt-saveResp').show();
		      }else if($(this).hasClass('bugSend') && $(this).not('.selected')){
		          $('.bugsWrap').animate({
		              left: '0'
		          }, 200);
		          $('.bt-saveResp').show();
		      }
		      $('.bugTabs').removeClass('selected');
		      $(this).addClass('selected');
		  });
		            
			//kh-tooltip
		  /*$(function(){
        	  $(".kh-tooltip").tipTip({
        		  defaultPosition: "top",
        		  edgeOffset: 5,
        		  fadeIn: 0,
        		  fadeOut: 0
        	  });
          });*/
		  $('.kh-tooltip').qtip({
			  position: {
                  corner: {
                	  target: 'bottomLeft',
                      tooltip: 'topRight'
                  }
              },
              style: { 
            	  border: {
         	         radius: 3,
                     color: '#e3ddef'
         	      },
          	      tip: { 
                      corner: 'topRight',
                      size: {x:5,y:5}
          	      },
                  name: 'light'
               },
               show: { delay: 1000, effect: 'fade' }
			});
          /* cores alternadas na tabela */
          $("#tab-admin tr:even").addClass("even");
          $("#tab-admin tr:odd").addClass("odd");
          $(".table-admin tr:even").addClass("even");
          $(".table-admin tr:odd").addClass("odd");
          $("#tab-admin-oc tr:even").addClass("even");
          $("#tab-admin-oc tr:odd").addClass("odd");
          /* centralizar colunas na tabela */
          /* modal */
          $('a.modal').fancybox({
      		'transitionIn'	:	'fade',
      		'transitionOut'	:	'fade',
      		'speedIn'		:	100,
      		'speedOut'		:	200,
      		'overlayOpacity' : 0,
      		'padding'       :   0,
      		'onComplete'	:	function() { $('.input-cliente').focus(); }
      	});

          /* adicionar linha (inventei esse $(this).parent().parent())*/
          /* $(".ico-edit").click(function(){
       		$($(this).parent().parent()).after('<tr><td colspan="9" class="details"><table cellspacing="0" border="0"><tr><td>Nome:</td><td><input type="text" style="width:640px;"/></td></tr><tr><td valign="top">Descrição:</td><td><textarea style="width: 640px; height: 70px;"></textarea></td></tr><tr><td colspan="2" style="text-align: right;"><button>Cancelar</button> <button>Salvar</button></td></tr></table></td></tr>');
          }); */
          /* Check all checkboxes*/

          $(".check_all").click(function(){
          	var checked_status = this.checked;
          	$(".optcheck").each(function(){
          		this.checked = checked_status;
          	});
		  });
          $('#tela').uploadify({
              'uploader':'/js/uploadify/uploadify.swf',
              'script': '/file/upload',
              'cancelImg':'/js/uploadify/cancel.png',
              'scriptAccess':'always',
              'buttonText':'anexar',
              'removeCompleted':false,
              'auto':true,
              'onComplete':function(event,id,file,data,info){
                  var data = $.parseJSON(data);
                  if(data){
                      fnnovoanexo();
                      var path = data.path.match(/\/files.*/);
                      var tipo = 0;
                      $(".opn-chmd-wrt-anex-lst").append('<a title="'+data.fileName+'" href="/file/view/id/'+data.id+'" class="linkanexo"\><span>'+data.fileName+'</span></a>');
                      var anexo = {
                          'id': data.id,
                          'pathTmp': path[0],
                          'fileName': data.fileName,
                          'fileType' : data.type,
                          'tipo': tipo
                      };
                      anexos.push(anexo);
                  }
              }
          });
          $('#menu-top .li-user a').live('click', function(){
      	    usrwidht = $('#menu-top .li-user .user').width();
      	    $('#menu-top .li-user .hide').css('width', usrwidht);
      		$('#menu-top .li-user .hide').slideToggle();
      		$(this).toggleClass("active");

      	  });
      	  $('#menu-top .li-user ul li a').live('click', function(){
      		$('#menu-top .li-user a').toggleClass("active");
      	  });
      });
</script>
